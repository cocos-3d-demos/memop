{
  "name": "memop",
  "version": "1.0.0",
  "description": "Visualizing and monitor your performance.",
  "main": "dist/memop",
  "scripts": {
    "build:dev": "rollup -c ./script/rollup.config.js",
    "build:min": "uglifyjs ./dist/memop.dev.js --mangle --source-map ./dist/memop.min.js.map -o ./dist/memop.min.js",
    "release": "npm run build:dev && npm run build:min",
    "start": "electron",
    "server": "preview -p 8002 -e examples",
    "dev": "rollup -w -c ./script/rollup.config.js",
    "test": "npm run build:dev && tap test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamedev-js/memop.git"
  },
  "keywords": [
    "memory",
    "data structure",
    "pool"
  ],
  "author": "jwu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gamedev-js/memop/issues"
  },
  "homepage": "https://github.com/gamedev-js/memop/issues",
  "dependencies": {},
  "devDependencies": {
    "electron": "^1.6.6",
    "fs-jetpack": "^0.13.0",
    "rollup": "^0.41.4",
    "rollup-watch": "^3.2.2",
    "tap": "^10.3.2",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  }
}
